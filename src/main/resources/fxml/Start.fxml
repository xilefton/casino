            fx:controller="ch.bbbaden.casino.scenes.StartController">
    <stylesheets>
        <URL value="@../styles/BasicStyle.css"/>
    </stylesheets>
    <ImageView fx:id="logo" fitHeight="175.0" fitWidth="241.0" layoutX="271.0" opacity="0.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../images/casino_logo.gif"/>
    </ImageView>
    <Pane fx:id="pane_buttons" layoutX="284.0" layoutY="207.0" opacity="0.0" prefHeight="200.0" prefWidth="200.0">
        <Button layoutX="25.0" layoutY="8.0" mnemonicParsing="false" onAction="#login" prefHeight="31.0"
                prefWidth="157.0" text="Login"/>
        <Button id="register" layoutX="25.0" layoutY="62.0" mnemonicParsing="false" onAction="#registrieren"
                prefHeight="31.0" prefWidth="157.0" text="Registrieren"/>
        <Button layoutX="25.0" layoutY="131.0" mnemonicParsing="false" onAction="#adminLogin" prefHeight="31.0"
                prefWidth="157.0" text="Admin Login"/>
    </Pane>
</AnchorPane>
